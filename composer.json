{
    "name": "imt/lexik",
    "type": "symfony-bundle",
    "description": "This bundle allows you to place your website in maintenance mode by calling two commands from your console.",
    "keywords": ["Symfony5", "bundle", "maintenance"],
    "homepage": "https://bitbucket.org/ArturTovmasyan",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Tovmasyan",
            "email": "arturt@intermotionllc.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/translation": "^4.0|^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0|^5.0",
        "phpunit/phpunit": "~4.8|~5.7.11"
    },
    "autoload":     {
        "psr-4": { "Lexik\\Bundle\\MaintenanceBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
